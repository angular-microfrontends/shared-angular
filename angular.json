{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm",
    "defaultCollection": "@angular-eslint/schematics"
  },
  "newProjectRoot": "projects",
  "projects": {
    "core": {
      "projectType": "library",
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "prefix": "core",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/core/tsconfig.lib.json",
            "project": "projects/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/core/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {}
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/core/**/*.ts",
              "projects/core/**/*.html"
            ]
          }
        }
      }
    },
    "angular-animations": {
      "projectType": "application",
      "root": "projects/angular-animations",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/angular-animations",
            "index": "common/repack/index.html",
            "main": "common/repack/main.ts",
            "tsConfig": "common/repack/tsconfig.app.json",
            "aot": true,
            "customWebpackConfig": {
              "path": "projects/angular-animations/extra-webpack.config.js",
              "libraryName": "angular-animations",
              "libraryTarget": "umd"
            }
          },
          "configurations": {
            "production": {
              "index": "",
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        }
      }
    },
